{
  "name": "formelio",
  "version": "0.0.45",
  "description": "Forms for human beings",
  "author": "Sergio Garcia Mondaray &lt;sgmonda@gmail.com&gt; (https://sgmonda.com)",
  "license": "GPL-3.0",
  "repository": "Sergio Garcia Mondaray &lt;sgmonda@gmail.com&gt; (https://sgmonda.com)/formelio",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "clean": "rm -Rf dist/* site/build/*",
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "lint": "eslint --fix -c .eslintrc . --ext .tsx,.ts,.json --ignore-path .gitignore",
    "dev": "microbundle-crl watch --no-compress --format modern,cjs",
    "dev:site": "npm run site:build && cd site && npm start",
    "dev:test": "react-scripts test --env=jsdom",
    "prepare": "husky install",
    "test": "run-s lint test:unit",
    "test:build": "run-s build",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:unit:reset": "npm run test:unit -- --updateSnapshot",
    "predeploy": "cd site && npm install && npm run build",
    "site": "npm run site:build && npm run site:deploy",
    "site:build": "cd site && npm install && npm run build",
    "site:deploy": "gh-pages -d site/build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@creativebulma/bulma-tooltip": "^1.2.0",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-jest": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addon-storyshots": "^6.2.9",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.2.9",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/classnames": "^2.2.11",
    "@types/faker": "^5.5.3",
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.168",
    "@types/node": "^12.12.38",
    "@types/react": "^16.9.27",
    "@types/react-datepicker": "^3.1.8",
    "@types/react-dom": "^16.9.7",
    "@types/react-select": "^4.0.13",
    "@types/styled-components": "^5.1.9",
    "@types/testing-library__jest-dom": "^5.9.5",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "babel-eslint": "^10.0.3",
    "bulma": "^0.9.2",
    "classnames": "^2.2.6",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "gulp-cli": "^2.3.0",
    "husky": "^5.2.0",
    "jest-extended": "^0.11.5",
    "lint-staged": "^10.5.4",
    "microbundle-crl": "^0.13.10",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-datepicker": "^3.6.0",
    "react-dom": "^16.13.1",
    "react-scripts": "^4.0.3",
    "react-select": "^4.3.0",
    "storybook-css-modules-preset": "^1.1.1",
    "typescript": "^3.7.5"
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,html,css,scss,md}": [
      "prettier --write --config .prettierrc"
    ],
    "*.{js,jsx,ts,tsx,json}": [
      "eslint --fix -c .eslintrc"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.35",
    "@fortawesome/free-solid-svg-icons": "5.15.3",
    "@fortawesome/react-fontawesome": "0.1.14",
    "@popperjs/core": "^2.9.2",
    "faker": "^5.5.3",
    "lodash": "^4.17.21",
    "react-datepicker": "3.6.0",
    "react-markdown": "^5.0.3",
    "react-select": "4.3.0",
    "styled-components": "^5.2.1"
  }
}
